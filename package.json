{
  "name": "vue-axe",
  "version": "0.0.1",
  "description": "Dynamic accessibility analysis for Vue.js using axe-core",
  "main": "dist/vue-axe.js",
  "scripts": {
    "dev": "rollup --config rollup.config.dev.js --watch",
    "dist": "rollup --config rollup.config.prod.js",
    "release": "standard-version",
    "projectPublish": "git push --follow-tags origin master && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vue-a11y/vue-axe.git"
  },
  "keywords": [
